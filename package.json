{
  "name": "form-builder",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/server.ts",
    "build": "vite build && tsc -p tsconfig.server.json",
    "start": "node dist/server/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "lint": "eslint src/ --ext .ts,.vue",
    "format": "prettier --write \"src/**/*.{ts,vue,css,json}\"",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^6.3.0",
    "axios": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.4.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pinia": "^2.3.0",
    "uuid": "^11.0.0",
    "vue": "^3.5.0",
    "vue-chartjs": "^5.3.0",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0",
    "zod": "^3.24.0",
    "@vueuse/core": "^12.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@playwright/test": "^1.49.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.0.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "postcss": "^8.4.0",
    "prettier": "^3.4.0",
    "prisma": "^6.3.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "vue-eslint-parser": "^9.0.0",
    "@vue/test-utils": "^2.4.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma",
      "esbuild",
      "vue-demi"
    ]
  }
}
